root = true
charset = utf-8
end_of_line = lf
insert_final_newline = true

[*.py]
indent_size = 4
indent_style = space

[*.h]
indent_size = 2
indent_style = space

[*.cpp]
indent_size = 2
indent_style = space

[*.toml]
indent_size = 2
indent_style = space

[*go]
indent_style = tab

[*.lua]
indent_size = 2
indent_style = space
